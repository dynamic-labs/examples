{
  "expo": {
    "name": "react-native-expo-dynamic-demo",
    "slug": "react-native-expo-dynamic-demo",
    "version": "1.0.0",
    "backgroundColor": "#E6F4FE",
    "orientation": "portrait",
    "icon": "./assets/images/dynamic-logo.png",
    "scheme": "reactnativeexpov2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/dynamic-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#E6F4FE"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.reactnativeexpov2",
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.com.yourcompany.reactnativeexpov2"
        ]
      },
      "infoPlist": {
        "NSApplePayCapabilityEnabled": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/dynamic-logo.png",
        "backgroundImage": "./assets/images/dynamic-logo.png",
        "monochromeImage": "./assets/images/dynamic-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.yourcompany.reactnativeexpov2"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/dynamic-logo.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#E6F4FE"
        }
      ],
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
